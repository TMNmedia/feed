<%- include('layout/header') %>

<div class="container">
    <h3>TMNMEDIA SIGNAL ALERT- REPORT</h3>
    <div class="row my-4">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                   
                    <div class="table-responsive">
                        <table class="table table-striped text-center">
                            <thead>
                                <tr class="table-dark">
                                    
                                    
                                    <th>Date</th>
                                    <th>Trade-Time</th>
                                    <th>Tick-Value</th>
                                    <th>Exit-Value</th>
                                    <th>Trade-Status</th>
                                    
                                </tr>
                            </thead>
                            <tbody>
                                <% x.forEach((y)=>{%>
                                <tr>
                                    <td><%= y.Date %></td>
                                    <td><%= y.AlertTime %></td>
                                    <td><%= y.EntryValue %></td>
                                    <td><%= y.TargetValue %></td>
                                    <td><%= y.TradeResult %></td>
                                    
                                </tr>
                                <%})%>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>

        
    </div>
    
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.7.0/jquery.min.js" integrity="sha512-3gJwYpMe3QewGELv8k/BX9vcqhryRdzRMxVfq6ngyWXwo03GFEzjsUm8Q7RZcHPHksttq7/GFoxjCVUjkjvPdw==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
<script src="https://cdn.datatables.net/v/bs4/dt-1.13.4/datatables.min.js"></script>
<script>
    $(document).ready(function () {
        $("table"). DataTable({
        order: [0, 'desc']
        });
    });
    //THIS HERE IS CODE FOR PAGINATION WITH JQUERY ON BOOTSTRAP 4
</script>

<%- include('layout/footer') %>

